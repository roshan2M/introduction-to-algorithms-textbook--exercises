package section65;

public class PriorityQueue<T extends Comparable<T>> {
	
	T[] array;
	int size;
	
	public PriorityQueue() {
		array = (T[]) new Comparable[2];
		size = 0;
	}
	
	public void insert(T t) {
		if (size == array.length - 1) resize(2*size + 1);
		array[size++] = t;
		max_heapify(size);
	}

	private void max_heapify(int size) {
		
	}

	private void resize(int capacity) {
		T[] copy = (T[]) new Comparable[capacity];
		for (int i = 0; i < size; i++) copy[i] = array[i];
		array = copy;
	}

}
